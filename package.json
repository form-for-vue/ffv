{
  "name": "ffv",
  "version": "0.3.21",
  "description": "Form For Vue",
  "author": "Siavash Kavousi",
  "main": "dist/ffv.common.js",
  "module": "dist/ffv.esm.js",
  "browser": "dist/ffv.js",
  "unpkg": "dist/ffv.js",
  "style": "dist/ffv.css",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "rollup -c --minify",
    "watch": "rollup -c --watch",
    "dev": "PORT=6060 nuxt",
    "lint": "yon run lint:js && yon run lint:css",
    "lint:js": "eslint --ext js --ext jsx --ext vue src test/**/*.spec.js test/*.js build",
    "lint:js:fix": "yon run lint:js --fix",
    "lint:css": "stylelint src/**/*.{vue,scss,css}",
    "lint:css:fix": "stylelint src/**/*.{scss,css}",
    "lint:staged": "lint-staged",
    "pretest": "yon run lint",
    "prepublish": "yon run build",
    "precommit": "lint:js:fix"
  },
  "lint-staged": {
    "*.{vue,jsx,js}": [
      "eslint --fix"
    ],
    "*.{vue,css}": [
      "stylefmt",
      "stylelint"
    ]
  },
  "pre-commit": "lint:staged",
  "devDependencies": {
    "@nuxtjs/bootstrap-vue": "^2.0.2",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-env": "^1.6.0",
    "bootstrap-vue": "^1.0.0-beta.9",
    "clean-css": "^4.0.0",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "beta",
    "lint-staged": "^4.1.2",
    "mkdirp": "^0.5.1",
    "nuxt": "^1.0.0-rc11",
    "postcss": "^6.0.0",
    "postcss-cssnext": "^3.0.2",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.0",
    "rollup": "^0.50.0",
    "rollup-plugin-alias": "^1.3.1",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-buble": "^0.16.0",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-jsx": "^1.0.3",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-postcss": "^0.5.5",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-plugin-vue": "^2.5.1",
    "rollup-watch": "^4.3.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.17.0",
    "stylefmt": "^6.0.0",
    "stylelint": "^8.1.1",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-processor-html": "^1.0.0",
    "uglify-es": "^3.1.3",
    "uglify-js": "^3.1.3",
    "uppercamelcase": "^3.0.0",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "yarn-or-npm": "^2.0.4"
  },
  "peerDependencies": {
    "vue": "^2.4.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/form-for-vue/ffv.git"
  },
  "bugs": {
    "url": "https://github.com/form-for-vue/ffv/issues"
  },
  "homepage": "https://github.com/form-for-vue/ffv#readme",
  "license": "MIT",
  "dependencies": {
    "@ffvjs/ajv-i18n": "^2.1.0",
    "ajv": "^5.4.0"
  },
  "optionalDependencies": {
    "vuedraggable": "^2.14.1"
  }
}
